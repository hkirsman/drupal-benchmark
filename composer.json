{
    "name": "wunderio/drupal-project",
    "version": "2.0",
    "description": "Wunder's template for Drupal 9 projects.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=8.1",
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.1",
        "drupal/bartik": "^1.0",
        "drupal/ckeditor": "^1.0",
        "drupal/core-composer-scaffold": "10.5.8",
        "drupal/core-recommended": "10.5.8",
        "drupal/monolog": "^2.0",
        "drupal/seven": "^1.0",
        "drupal/simplei": "^2.1",
        "drupal/warden": "^4.0@beta",
        "drush/drush": "^12.0",
        "vlucas/phpdotenv": "^5.4",
        "webflo/drupal-finder": "^1.2",
        "wunderio/drupal-ping": "^2.5"
    },
    "require-dev": {
        "drupal/core-dev": "10.5.8",
        "phpspec/prophecy-phpunit": "^2.0",
        "wunderio/code-quality": "^2.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "allow-plugins": {
            "composer/installers": true,
            "composer/package-versions-deprecated": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "koodimonni/composer-dropin-installer": true,
            "php-http/discovery": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        },
        "audit": {
            "block-insecure": false
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "dropin-paths": {
            "web/": ["type:web-dropin"]
        }
    }
}
